{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/custom"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "BUN_INSTALL": "1",
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "BUN_INSTALL": "1",
      "NODE_ENV": "production"
    }
  }
}
